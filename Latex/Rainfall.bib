file = {:C\:/Users/febri/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sato, Oka, Kato - 2019 - Early Turnover Prediction of New Restaurant Employees from Their Attendance Records and Attributes.pdf:pdf},
