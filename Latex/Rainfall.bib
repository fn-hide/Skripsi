title = {{A comparative study of LSTM and ARIMA for energy load prediction with enhanced data preprocessing}},
year = {2020}
}
@article{Fauzi2018,
abstract = {Intensity of precipitation in Indonesia is influenced by convection and propagation of southwest wind. Objective of this study is to analyze the relationship between cold surge and the phenomenon of intra-seasonal climate variability Madden-julian Oscillation (MJO) for affecting precipitation in Indonesia. The data used for identifying the occurrence of cold surge are meridional wind speed data from the ERA-Interim. In addition, this study also used RMM1 and RMM2 index data from Bureau of Meteorology (BOM) for identifying MJO events. The results showed that during East Asian Winter Monsoon (EAWM) in 15 years (2000-2015), there are 362 cold surge events, 186 MJO events, and 113 cold surge events were associated with MJO events. The spread of cold surge can penetrate to equator and brought mass of water vapor that causes dominant precipitation in the Indonesian Sea up to 50-75% from climatological precipitation during EAWM. The MJO convection activity that moves from west to east also increases precipitation, but the distribution of rainfall is wider than cold surge, especially in Eastern Indonesia. MJO and cold surge simultaneously can increase rainfall over 100-150% in any Indonesian region that affected by MJO and cold surge events. The mechanism of heavy rainfall is illustrated by high activity of moisture transport in areas such as Java Sea and coastal areas of Indonesia.},
author = {Fauzi, R. R. and Hidayat, R.},
doi = {10.1088/1755-1315/149/1/012045},
file = {:C\:/Users/febri/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fauzi, Hidayat - 2018 - Role of Cold Surge and MJO on Rainfall Enhancement Over Indonesia During East Asian Winter Monsoon.pdf:pdf},
