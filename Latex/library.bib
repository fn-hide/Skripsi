file = {:C\:/Users/febri/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dutta, Anjum - 2021 - Optimization of Temperature and Relative Humidity in an Automatic Egg Incubator Using Mamdani Fuzzy Inference Syst.pdf:pdf},
isbn = {9781665415767},
journal = {International Conference on Robotics, Electrical and Signal Processing Techniques},
keywords = {Mamdani,Takagi-Sugeno,artificial intelligence,control system,fuzzy logic,humidity,incubator,sensor,temperature},
