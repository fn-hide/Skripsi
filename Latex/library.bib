keywords = {covid-19 cost-effectiveness analysis,medical treatment,modeling,optimal control,quarantine and testing},
number = {Tobias},
pages = {1--22},
title = {{Controlling COVID-19 spread in Jakarta, Indonesia, using quarantine, testing and medical treatment}},
year = {2021}
}
