issn = {2410387X},
journal = {Cryptography},
keywords = {Internet of Things (IoT),YOLO network,accelerator-based SBCs,edge computing,intelligence edge},
number = {2},
pages = {1--16},
title = {{Benchmark Analysis of YOLO Performance on Edge Intelligence Devices}},
volume = {6},
year = {2022}
}
