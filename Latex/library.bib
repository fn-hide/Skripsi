journal = {Pattern Recognition and Image Analysis},
keywords = {classifier,computed tomography,hepatocellular carcinoma,liver cancer,metastatic carcinoma,segmentation},
number = {2},
pages = {201--211},
title = {{Detection of Liver Cancer Using Modified Fuzzy Clustering and Decision Tree Classifier in CT Images}},
volume = {29},
year = {2019}
}
