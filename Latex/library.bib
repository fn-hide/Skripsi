author = {Joshi, Chaitanya K. and Laurent, Thomas and Bresson, Xavier},
eprint = {1906.01227},
file = {:C\:/Users/febri/Downloads/1906.01227.pdf:pdf},
pages = {1--17},
title = {{An Efficient Graph Convolutional Network Technique for the Travelling Salesman Problem}},
url = {http://arxiv.org/abs/1906.01227},
year = {2019}
}
