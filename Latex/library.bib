file = {:C\:/Users/febri/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Farsi et al. - 2021 - On Short-Term Load Forecasting Using Machine Learning Techniques and A Novel Parallel Deep LSTM-CNN Approach.pdf:pdf},
issn = {21693536},
journal = {IEEE Access},
keywords = {Convolutional neural networks,Deep learning,Electricity,Load consumption,Long short-term memory,Regression,Short-term load forecasting,Smart grids,Time series},
pages = {31191--31212},
title = {{On Short-Term Load Forecasting Using Machine Learning Techniques and A Novel Parallel Deep LSTM-CNN Approach}},
volume = {9},
year = {2021}
}
