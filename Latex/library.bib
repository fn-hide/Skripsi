journal = {Water (Switzerland)},
keywords = {Flood events,LSTM,Rainfall-runoff},
month = {oct},
number = {11},
publisher = {MDPI AG},
title = {{Deep Learning With A Long Short-Term Memory Networks Approach for Rainfall-Runoff Simulation}},
volume = {10},
year = {2018}
