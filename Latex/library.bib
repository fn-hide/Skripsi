keywords = {Feature extraction,Fuzzy C-means clustering,Gray level intensity,Magnetic resonance imaging,T-means clustering,Template-based K-means and modified fuzzy C-means},
number = {2},
pages = {1--18},
title = {{Automatic human brain tumor detection in mri image using template-based k means and improved fuzzy c means clustering algorithm}},
